
#include <iostream> // when summing ANYTHING , let the original value be equal to 0, and use the compound operators!!!!!!!!!!!!!!!!!!!!!!!!!!!!
#include <cmath>
#include <string>
using namespace std;
int main()
{
    int year = 2000;
    for (year; year<=2100; year++)
    {
        if (year == 2000)
        {
            continue;
        }
        if (year%4 == 0 && year !=2000)
            {cout<<" "<< year << " ";}
    }
    cout << " "<<"are leap years";
    return 0;
}
