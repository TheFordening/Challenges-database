#include "pch.h"
#include <iostream>
#include <cmath>
#include <string>
#include <fstream>
#include <istream>
using namespace std;
int main()
{
	ofstream Myfile2("countletters.txt");
	cout << " Without code you dead!.\n";
	Myfile2.close();
	string line;
	ifstream Myfile("countletters.txt");
	if (Myfile.is_open())
	{
		while (getline(Myfile, line))
		{
			cout << line << '\n';
		}
		Myfile.close();

	}
	else
	{
		cout << "It is not open.\n";
	}

}
